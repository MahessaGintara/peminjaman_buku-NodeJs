doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title #{title}
    include header.pug 

  body.hold-transition.skin-blue.sidebar-mini 
    div.wrapper
      include navigation.pug
      include sidebar.pug
      .content-wrapper
        block content
      include footer.pug
      include js.pug